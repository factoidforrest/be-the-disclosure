{
  "compilerOptions": {
    "lib": ["ESNext"],
    "target": "ESNext", // Adjust according to your Node.js version
    "module": "CommonJS",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true
  }
}
